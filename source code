									printf("\n     THE PERCENT OF AFFECTED IS %f WHICH IS MORE THAN 25 PERCENT",percent);
									printf("\n\t\t THUS E-PASS TO COIMBATORE IS NOT PERMISSIBLE");
									getch();
									EPASS();

								}
								break;
						}
						break;

					case 2:
						to_dist=GET_DIST(to_state);
						switch(to_dist)
						{
							case 1:
								fp=fopen("ernakulam.txt","r");
								fscanf(fp,"%d %d %d %d %d %d %d %f",&lvl1,&lvl2,&lvl3,&cured,&death,&vent,&population,&percent);
