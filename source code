void VENTILATOR_STATUS(void)
{
   int lvl1,lvl2,lvl3,vent,cured,death,tot_lvl3=0,tot_vent=0;
fp=fopen("madurai.txt","r");
   if(fp==NULL)
printf("\n REQUEST FOR OPENING FILE MADURAI IS DECLINED");
   else
   {
fscanf(fp,"%d %d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death,&vent);
tot_vent=tot_vent+vent;
      tot_lvl3=tot_lvl3+lvl3;
fclose(fp);
   }
fp=fopen("nagapattinam.txt","r");
   if(fp==NULL)
printf("\n REQUEST FOR OPENING FILE NAGAPATTINAM IS DECLINED");
   else
   {
fscanf(fp,"%d %d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death,&vent);
tot_vent=tot_vent+vent;
      tot_lvl3=tot_lvl3+lvl3;
fclose(fp);
   }
fp=fopen("coimbatore.txt","r");
   if(fp==NULL)
printf("\n REQUEST FOR OPENING FILE COIMBATORE IS DECLINED");
   else
   {
fscanf(fp,"%d %d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death,&vent);
tot_vent=tot_vent+vent;
      tot_lvl3=tot_lvl3+lvl3;
fclose(fp);
   }
   if(tot_lvl3<tot_vent)

printf("\n THERE  ARE ENOUGH VENTILATORS IN TAMILNADU");
   else
   {
printf("\n ALARM!!!! TAMILNADU HAS A SCARCITY OF VENTILATORS");
printf("\n %d VENTILATORS ARE REQUIRED IN TAMILNADU",tot_lvl3-tot_vent);
   }
   tot_lvl3=0;
tot_vent=0;
fp=fopen("ernakulam.txt","r");
   if(fp==NULL)
printf("\n REQUEST FOR OPENING FILE ERNAKULAM IS DECLINED");
   else
   {
fscanf(fp,"%d %d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death,&vent);
tot_vent=tot_vent+vent;
      tot_lvl3=tot_lvl3+lvl3;
fclose(fp);
   }
fp=fopen("alappuzha.txt","r");
   if(fp==NULL)
printf("\n REQUEST FOR OPENING FILE ALAPPUZHA IS DECLINED");
   else
   {
fscanf(fp,"%d %d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death,&vent);
tot_vent=tot_vent+vent;
      tot_lvl3=tot_lvl3+lvl3;
fclose(fp);
   }
fp=fopen("kollam.txt","r");
   if(fp==NULL)
printf("\n REQUEST FOR OPENING FILE KOLLAM IS DECLINED");
   else
   {
fscanf(fp,"%d %d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death,&vent);
tot_vent=tot_vent+vent;
      tot_lvl3=tot_lvl3+lvl3;
fclose(fp);
   }
   if(tot_lvl3<tot_vent)
printf("\n THERE  ARE ENOUGH VENTILATORS IN KERALA");
   else
   {
printf("\n ALARM!!!! KERALA HAS A SCARCITY OF VENTILATORS");
printf("\n %d VENTILATORS ARE REQUIRED IN KERALA",tot_lvl3-tot_vent);
   }
   tot_lvl3=0;
tot_vent=0;
fp=fopen("kolar.txt","r");
   if(fp==NULL)
printf("\n REQUEST FOR OPENING FILE KOLAR IS DECLINED");
   else
   {
fscanf(fp,"%d %d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death,&vent);
tot_vent=tot_vent+vent;
      tot_lvl3=tot_lvl3+lvl3;
fclose(fp);
   }
fp=fopen("bagalkot.txt","r");
   if(fp==NULL)
printf("\n REQUEST FOR OPENING FILE BAGALKOT IS DECLINED");
   else
   {
fscanf(fp,"%d %d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death,&vent);
tot_vent=tot_vent+vent;
      tot_lvl3=tot_lvl3+lvl3;
fclose(fp);
   }
fp=fopen("belagari.txt","r");
   if(fp==NULL)
printf("\n REQUEST FOR OPENING FILE BELAGARI IS DECLINED");
   else
   {
fscanf(fp,"%d %d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death,&vent);
tot_vent=tot_vent+vent;
      tot_lvl3=tot_lvl3+lvl3;
fclose(fp);
   }
   if(tot_lvl3<tot_vent)
printf("\n THERE  ARE ENOUGH VENTILATORS IN KARNATAKA");
   else
   {
printf("\n ALARM!!!! KARNATAKA HAS A SCARCITY OF VENTILATORS");
printf("\n %d VENTILATORS ARE REQUIRED IN KARNATAKA",tot_lvl3-tot_vent);
   }
getch();
}

int main()
{
  int ch;
ASSIGN();
while(1)
  {
	clrscr();
	printf("\n---------CORONA INFORMATION RECORD SYSTEM---------");
	printf("----------------------------------------------------");
	printf("MENU :\n1.STATE COMPARISON\n2.DISTRICT COMPARISON\n3.VENTILATOR STATUS\n4.DATA INPUT\n5.INFORMATION\n6.EXIT");
	printf("------------------------------------------------------------------------------------------------------------");
	printf("\n ENTER YOUR CHOICE : ");
	scanf("%d",&ch);
	switch(ch)
	{
		case 3:
		       VENTILATOR_STATUS();
		       break;
		case 4:
		READDATA();
		       break;
		case 5:
			INFORMATION();
			break;
		case 6:
			printf("\n THANK YOU FOR USING CORONA INFORMATION SYSTEM.\nHOPE IT WAS USEFUL TO YOU.");
			break;
		default:printf("\n PLEASE ENTER A CHOICE NUMBER BETWEEN 1-6");
	}
	if(ch==6)
		break;
  }
getch();
return(0);
}
