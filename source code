void INFORMATION(void)
{
    int lvl1,lvl2,lvl3,cured,death,vent,state,dist;
clrscr();
printf("-----------------------------------\n");
printf("1.TAMILNADU\n2.KERALA\n3.KARNATAKA\n");
printf("-----------------------------------\n");
printf("ENTER STATE ID: ");
scanf("%d",&state);
    switch(state)
    {
       case 1 :
		clrscr();
		printf("\n------------------------------------");
		printf("\n\t\t\tSTATE 1 : TAMIL NADU");
		printf("\n------------------------------------");
		printf("\n1.MADURAI\n2.NAGAPATTINAM\n3.COIMBATORE\n4.ALL");
		printf("\n---------------------------------------");
		printf("\n ENTER DISTRICT ID : ");
		scanf("%d",&dist);
		switch(dist)
		{
		   case 1:
			clrscr();
			printf("\t\t\t STATE 1 : TAMILNADU---DISTRICT 1: MADURAI");
			fp=fopen("madurai.txt","r");
			   if(fp==NULL)
			printf("\n REQUEST FOR OPENING FILE MADURAI IS DECLINED");
			   else
			   {
			fscanf(fp,"%d %d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death,&vent);
			printf("\n THE NUMBER OF PATIENTS IN LEVEL 1 ARE : %d",lvl1);
			printf("\n THE NUMBER OF PATIENTS IN LEVEL 2 ARE : %d",lvl2);
			printf("\n THE NUMBER OF PATIENTS IN LEVEL 3 ARE : %d",lvl3);
			printf("\n THE NUMBER OF PATIENTS RECOVERED ARE : %d",cured);
			printf("\n THE NUMBER OF PATIENTS DEAD ARE : %d",death);
			printf("\n THE NUMBER OF VENTILATORS IS : %d",vent);
			getch();
			fclose(fp);
			   }
			   break;
		   case 2:
			clrscr();
			printf("\t\t\t STATE 1 : TAMILNADU---DISTRICT 2: NAGAPATTINAM");
			fp=fopen("nagapattinam.txt","r");
			   if(fp==NULL)
			printf("\n REQUEST FOR OPENING FILE NAGAPATTINAM IS DECLINED");
			   else
			   {
			fscanf(fp,"%d %d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death,&vent);
			printf("\n THE NUMBER OF PATIENTS IN LEVEL 1 ARE : %d",lvl1);
			printf("\n THE NUMBER OF PATIENTS IN LEVEL 2 ARE : %d",lvl2);
			printf("\n THE NUMBER OF PATIENTS IN LEVEL 3 ARE : %d",lvl3);
			printf("\n THE NUMBER OF PATIENTS RECOVERED ARE : %d",cured);
			printf("\n THE NUMBER OF PATIENTS DEAD ARE : %d",death);
			printf("\n THE NUMBER OF VENTILATORS IS : %d",vent);
			getch();
			fclose(fp);
			   }
			   break;
		   case 3:
			clrscr();
			printf("\t\t\t STATE 1 : TAMILNADU---DISTRICT 3: COIMBATORE");
			fp=fopen("coimbatore.txt","r");
			   if(fp==NULL)
				printf("\n REQUEST FOR OPENING FILE COIMBATORE IS DECLINED");
			   else
			   {
