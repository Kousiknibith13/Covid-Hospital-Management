void ASSIGN(void);
FILE * fp;
void ASSIGN(void)
{
   int lvl1=50,lvl2=20,lvl3=10,cured=20,death=5,vent=20;
fp=fopen("madurai.txt","w+");
   if(fp==NULL)
   {
printf("\n REQUEST FOR OPENING FILE MADURAI IS DECLINED");
   }
   else
   {
fprintf(fp,"%d",lvl1);
fprintf(fp,"\n");
fprintf(fp,"%d",lvl2);
fprintf(fp,"\n");
fprintf(fp,"%d",lvl3);
fprintf(fp,"\n");
fprintf(fp,"%d",cured);
fprintf(fp,"\n");
fprintf(fp,"%d",death);
fprintf(fp,"\n");
fprintf(fp,"%d",vent);
fclose(fp);
   }
fp=fopen("nagapattinam.txt","w+");
   if(fp==NULL)
printf("\n REQUEST FOR OPENING FILE NAGAPATTINAM IS DECLINED");
