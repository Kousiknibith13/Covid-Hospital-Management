#include<stdio.h>
#include<conio.h>
#include<graphics.h>
void READDATA (void);
void INFORMATION(void);
void VENTILATOR_STATUS(void);
void STATE_BAR_CHART(void);
void DISTRICT_BAR_CHART(void);
void BAR_CHART(int arr[3][5],int);
void ASSIGN(void);
void FILE_WRITE_DATA(void);
void INFO_MESSAGE(void);
int INFO_PRINT(void);
void GET_INFO(void);
int GET_DIST_ID(int);
void EPASS(void);
void E_PASS_FORM(void);
void E_PASS_DETAILS(void);
FILE * fp;
int state,dist,lvl1,lvl2,lvl3,death,cured,vent,population,num=1;
float percent;
char name[100],number[12],email[25],dest_dist[30],native[30];
void E_PASS_DETAILS(void)
{
	char temp[100];
	int i=1;
	clrscr();
	printf("\n--------------------------------------------------------------------------------");
	printf("\n\t\t\t   E-PASS-APPLICANTS-DETAILS:");
	printf("\n--------------------------------------------------------------------------------");
	fp=fopen("epasslst.txt","r");
	while(i<num)
	{
		fscanf(fp,"%s %s %s %s %s",name,email,number,native,dest_dist);
		printf("\nNAME : %s",name);
		printf("\nEMAIL : %s",email);
		printf("\nMOBILE NUMBER : %s",number);
		printf("\nNATIVE DISTRICT : %s",native);
		printf("\nDESTINATION : %s",dest_dist);
		printf("\n--------------------------------------------------------------------------------");
		getch();
		i++;
	}
	fclose(fp);
	getch();
}
void E_PASS_FORM(void)
{
     clrscr();

     printf("\n\t\t\t\tE-PASS-APPLICATION");
     printf("\n1.ENTER YOUR NAME : ");
     scanf("%s",name);
     printf("\n2.ENTER YOUR EMAIL-ID : ");
     scanf("%s",email);
     printf("\n3.ENTER YOUR PHONE NUMBER : ");
     scanf("%s",number);
     printf("\n4.ENTER YOUR DISTRICT : ");
     scanf("%s",native);
     printf("\n5.ENTER YOUR DESTINATION DISTRICT : ");
     scanf("%s",dest_dist);
     fp=fopen("epasslst.txt","a");
     fprintf(fp,"%d.%s",num,name);
     fprintf(fp,"\n");
     fprintf(fp,"%s",email);
     fprintf(fp,"\n");
     fprintf(fp,"%s",number);
     fprintf(fp,"\n");
     fprintf(fp,"%s",native);
     fprintf(fp,"\n");
     fprintf(fp,"%s",dest_dist);
     fprintf(fp,"\n");
     fclose(fp);
     num++;
}
int GET_DIST(int state)
{
    switch(state)
    {
	case 1:
		printf("\n\t\t\tSTATE 1 : TAMIL NADU\n");
		printf("--------------------------------------------------------------------------------");
		printf("\n1.MADURAI\n2.NAGAPATTINAM\n3.COIMBATORE\n");
		printf("--------------------------------------------------------------------------------");
		printf("\nENTER A DISTRICT ID : ");
		scanf("%d",&dist);
		return(dist);

	case 2:
		printf("\n\t\t\tSTATE 2 : KERALA\n");
		printf("--------------------------------------------------------------------------------");
		printf("\n1.ERNAKUALM\n2.ALAPPUZHA\n3.KOLLAM\n");
		printf("--------------------------------------------------------------------------------");
		printf("\nENTER A DISTRICT ID : ");
		scanf("%d",&dist);
		return(dist);

	case 3:
		printf("\n\t\t\tSTATE 3 : KARNATAKA\n");
		printf("--------------------------------------------------------------------------------");
		printf("\n1.KOLAR\n2.BAGALKOT\n3.BELAGARI\n");
		printf("--------------------------------------------------------------------------------");
		printf("\nENTER A DISTRICT ID : ");
		scanf("%d",&dist);
		return(dist);

	default:
		printf("\n ENTER A VALID DISTRICT ID FROM 1-3");
		getch();

		dist=GET_DIST(state);
		return(0);
    }
}
void GET_INFO(void)
{
   int temp;
   printf("\n ENTER THE NUMBER OF PATIENTS RECORDED IN LEVEL 1 : ");
   scanf("%d",&lvl1);
   printf("\n ENTER THE NUMBER OF PATIENTS RECORDED IN LEVEL 2 : ");
   scanf("%d",&lvl2);
   printf("\n ENTER THE NUMBER OF PATIENTS RECORDED IN LEVEL 3 : ");
   scanf("%d",&lvl3);
   printf("\n ENTER THE NUMBER OF PATIENTS CURED : ");
   scanf("%d",&cured);
   printf("\n ENTER THE NUMBER OF PATIENTS DEAD : ");
   scanf("%d",&death);
   printf("\n ENTER THE NUMBER OF VENTILATORS AVAILABLE : ");
   scanf("%d",&vent);
   printf("\n ENTER THE POPULATION OF THE DISTRICT : ");
   scanf("%d",&population);
   temp=lvl1+lvl2+lvl3;
   percent=temp*100;
   percent=percent/(float)population;
   printf("%f",percent);
   getch();

}
void INFO_MESSAGE(void)
{
   fscanf(fp,"%d %d %d %d %d %d %d %f",&lvl1,&lvl2,&lvl3,&cured,&death,&vent,&population,&percent);
   printf("\n THE NUMBER OF PATIENTS IN LEVEL 1 ARE : %d",lvl1);
   printf("\n THE NUMBER OF PATIENTS IN LEVEL 2 ARE : %d",lvl2);
   printf("\n THE NUMBER OF PATIENTS IN LEVEL 3 ARE : %d",lvl3);
   printf("\n THE NUMBER OF PATIENTS RECOVERED ARE : %d",cured);
   printf("\n THE NUMBER OF PATIENTS DEAD ARE : %d",death);
   printf("\n THE NUMBER OF VENTILATORS IS : %d\n",vent);
   printf("\n THE POPULATION OF THE DISTRICT IS : %d\n",population);
   printf("\n THE PERCENTAGE OF AFFECTED PATIENTS IN THE DISTRICT IS : %.2f\n",percent);
   printf("--------------------------------------------------------------------------------");

}
int INFO_PRINT(void)
{

    clrscr();
    printf("--------------------------------------------------------------------------------");
    printf("1.TAMILNADU\n2.KERALA\n3.KARNATAKA\n");
    printf("--------------------------------------------------------------------------------");
    printf("ENTER STATE ID: ");
    scanf("%d",&state);
    return state;

}
void FILE_WRITE_DATA(void)
{
      fprintf(fp,"%d",lvl1);
      fprintf(fp,"\n");
      fprintf(fp,"%d",lvl2);
      fprintf(fp,"\n");
      fprintf(fp,"%d",lvl3);
      fprintf(fp,"\n");
      fprintf(fp,"%d",cured);
      fprintf(fp,"\n");
      fprintf(fp,"%d",death);
      fprintf(fp,"\n");
      fprintf(fp,"%d",vent);
      fprintf(fp,"\n");
      fprintf(fp,"%d",population);
      fprintf(fp,"\n");
      fprintf(fp,"%f",percent);
}

void EPASS(void)
{
    int from_state,to_state,from_dist,to_dist;
    printf("\n\t\t\t   E-PASS ELIGIBILITY TESTING");
    from_state=INFO_PRINT();
    switch(from_state)
    {
	case 1:
		from_dist=GET_DIST(1);
		switch(from_dist)
		{
			case 1:
				fp=fopen("madurai.txt","r");
				fscanf(fp,"%d %d %d %d %d %d %d %f",&lvl1,&lvl2,&lvl3,&cured,&death,&vent,&population,&percent);
				fclose(fp);
				if(percent<25.0)
				{
				   printf("\n     THE PERCENT OF AFFECTED PATIENTS IN MADURAI IS %.2f WHICH IS LESS THAN 25 PERCENT",percent);
				   printf("\n\t    THUS THE E-PASS TO THE CITIZENS OF MADURAI IS PERMISSIBLE");
				   printf("\n\t\t ENTER THE STATE ID OF YOUR DESTINATION DISTRICT");
				   getch();
				   to_state=INFO_PRINT();
				   switch(to_state)
				   {
					case 1:
						to_dist=GET_DIST(1);
						switch(to_dist)
						{
							case 1:
								fp=fopen("madurai.txt","r");
								fscanf(fp,"%d %d %d %d %d %d %d %f",&lvl1,&lvl2,&lvl3,&cured,&death,&vent,&population,&percent);
								fclose(fp);
								if(percent<25.0)
								{
									printf("\n     THE PERCENT OF AFFECTED PATIENTS IN MADURAI IS %.2f WHICH IS LESS THAN 25 PERCENT",percent);
									printf("\n\t\t    THUS THE E-PASS TO MADURAI IS PERMISSIBLE");
									printf("\n\t\t\t  PLEASE FILL THE E-PASS FORM");
									getch();
									E_PASS_FORM();
								}
								else
								{
									printf("\n     THE PERCENT OF AFFECTED IS %f WHICH IS MORE THAN 25 PERCENT",percent);
									printf("\n\t\t THUS E-PASS TO MADURAI IS NOT PERMISSIBLE");
									getch();
									EPASS();

								}
								break;

							case 2:
								fp=fopen("nagapattinam.txt","r");
								fscanf(fp,"%d %d %d %d %d %d %d %f",&lvl1,&lvl2,&lvl3,&cured,&death,&vent,&population,&percent);
								fclose(fp);
								if(percent<25.0)
								{
									printf("\n     THE PERCENT OF AFFECTED PATIENTS IN NAGAPATTINAM IS %.2f WHICH IS LESS THAN 25 PERCENT",percent);
									printf("\n\t\t\tTHUS THE E-PASS TO NAGAPATTINAM IS PERMISSIBLE");
									printf("\n\t\t\t  PLEASE FILL THE E-PASS FORM");
									getch();
									E_PASS_FORM();
								}
								else
								{
									printf("\n     THE PERCENT OF AFFECTED IS %f WHICH IS MORE THAN 25 PERCENT",percent);
									printf("\n\t\t THUS E-PASS TO NAGAPATTINAM IS NOT PERMISSIBLE");
									getch();
									EPASS();

								}
								break;

							case 3:
								fp=fopen("coimbatore.txt","r");
								fscanf(fp,"%d %d %d %d %d %d %d %f",&lvl1,&lvl2,&lvl3,&cured,&death,&vent,&population,&percent);
