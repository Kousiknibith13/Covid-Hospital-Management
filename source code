			fprintf(fp,"\n");
			fprintf(fp,"%d",death);
			fprintf(fp,"\n");
			fprintf(fp,"%d",vent);
			fprintf(fp,"\n");
			fclose(fp);
			   }
			   break;
		   case 2:
			clrscr();
			printf("\t\t\t STATE 1 : TAMILNADU---DISTRICT 2: NAGAPATTINAM");
			printf("\n ENTER THE NUMBER OF PATIENTS RECORDED IN LEVEL 1 : ");
			scanf("%d",&lvl1);
			printf("\n ENTER THE NUMBER OF PATIENTS RECORDED IN LEVEL 2 : ");
			scanf("%d",&lvl2);
			printf("\n ENTER THE NUMBER OF PATIENTS RECORDED IN LEVEL 3 : ");
			scanf("%d",&lvl3);
			printf("\n ENTER THE NUMBER OF PATIENTS CURED : ");
			scanf("%d",&cured);
			printf("\n ENTER THE NUMBER OF PATIENTS DEAD : ");
			scanf("%d",&death);
			printf("\n ENTER THE NUMBER OF VENTILATORS AVAILABLE : ");
			scanf("%d",&vent);
			fp=fopen("nagapattinam.txt","w+");
			   if(fp==NULL)
			printf("\n REQUEST FOR OPENING FILE NAGAPATTINAM IS DECLINED");
			   else
			   {
			fprintf(fp,"%d",lvl1);
			fprintf(fp,"\n");
			fprintf(fp,"%d",lvl2);
			fprintf(fp,"\n");
			fprintf(fp,"%d",lvl3);
			fprintf(fp,"\n");
			fprintf(fp,"%d",cured);
			fprintf(fp,"\n");
			fprintf(fp,"%d",death);
			fprintf(fp,"\n");
			fprintf(fp,"%d",vent);
			fprintf(fp,"\n");
			fclose(fp);
			   }
			   break;
		   case 3:
			clrscr();
			printf("\t\t\t STATE 1 : TAMILNADU---DISTRICT 3: COIMBATORE");
			printf("\n ENTER THE NUMBER OF PATIENTS RECORDED IN LEVEL 1 : ");
			scanf("%d",&lvl1);
			printf("\n ENTER THE NUMBER OF PATIENTS RECORDED IN LEVEL 2 : ");
			scanf("%d",&lvl2);
			printf("\n ENTER THE NUMBER OF PATIENTS RECORDED IN LEVEL 3 : ");
			scanf("%d",&lvl3);
			printf("\n ENTER THE NUMBER OF PATIENTS CURED : ");
			scanf("%d",&cured);
			printf("\n ENTER THE NUMBER OF PATIENTS DEAD : ");
			scanf("%d",&death);
			printf("\n ENTER THE NUMBER OF VENTILATORS AVAILABLE : ");
			scanf("%d",&vent);
			fp=fopen("coimbatore.txt","w+");
			   if(fp==NULL)
			printf("\n REQUEST FOR OPENING FILE COIMBATORE IS DECLINED");
			   else
			   {
			fprintf(fp,"%d",lvl1);
			fprintf(fp,"\n");
			fprintf(fp,"%d",lvl2);
			fprintf(fp,"\n");
			fprintf(fp,"%d",lvl3);
			fprintf(fp,"\n");
			fprintf(fp,"%d",cured);
			fprintf(fp,"\n");
			fprintf(fp,"%d",death);
			fprintf(fp,"\n");
			fprintf(fp,"%d",vent);
			fprintf(fp,"\n");
			fclose(fp);
			   }
			   break;
		   default: printf("\n ENTER A VALID DISTRICT ID BETWEEN 1-3");
		}
		break;
       case 2:
		clrscr();
		printf("\n------------------------------------");
		printf("\n\t\t\tSTATE 2 : KERALA");
		printf("\n------------------------------------");
		printf("\n1.ERNAKULAM\n2.ALAPPUZHA\n3.KOLLAM");
