								}
								else
								{
									printf("\n     THE PERCENT OF AFFECTED IS %.2f WHICH IS MORE THAN 25 PERCENT",percent);
									printf("\n\t\t THUS E-PASS TO BAGALKOT IS NOT PERMISSIBLE");
									getch();
									EPASS();

								}
								break;

							case 3:
								fp=fopen("belagari.txt","r");
								fscanf(fp,"%d %d %d %d %d %d %d %f",&lvl1,&lvl2,&lvl3,&cured,&death,&vent,&population,&percent);
								fclose(fp);
								if(percent<25.0)
								{
									printf("\n     THE PERCENT OF AFFECTED PATIENTS IN BELAGARI IS %.2f WHICH IS LESS THAN 25 PERCENT",percent);
									printf("\n\t\t\t THUS THE E-PASS TO BELAGARI IS PERMISSIBLE");
									printf("\n\t\t\t  PLEASE FILL THE E-PASS FORM");
									getch();
									E_PASS_FORM();
								}
								else
								{
									printf("\n     THE PERCENT OF AFFECTED IS %.2f WHICH IS MORE THAN 25 PERCENT",percent);
									printf("\n\t\t THUS E-PASS TO BELAGARI IS NOT PERMISSIBLE");
									getch();
									EPASS();

								}
								break;
						}
						break;
				   }
			       }
			       else
			       {
					printf("\nTHE PERCENT OF AFFETCED PATIENTS IS %.2f WHICH IS MORE THAN 25 PERCENT",percent);
					printf("\n THUS E-PASS TO THE CITIZENS OF BELAGARI IS DENIED");
					getch();
			       }
			       break;

		}

    }

}
void BAR_CHART(int arr[3][5],int key)
{

    int gd=DETECT,gm;
    int x_bottom,x_top,y_top,y_bottom,pat,col,i,j;
    int bar_outline_x1,bar_outline_y1,bar_outline_x,bar_outline_y;
    int pattern[5]={1,7,10,9,11};
    int color[5]={1,4,2,5,6};
    clrscr();
    initgraph(&gd,&gm,"C:\\TURBOC3\\BGI");
    setcolor(YELLOW);
    rectangle(0,30,639,450);
    setlinestyle(SOLID_LINE,0,2);
    line(100,420,100,60);
    line(100,420,600,420);
    line(90,70,100,60);
    line(110,70,100,60);
    line(590,410,600,420);
    line(590,430,600,420);
    settextstyle(SMALL_FONT,HORIZ_DIR,5);
    outtextxy(530,100,"KEY");
    outtextxy(500,120,"1.BLUE - LEVEL1");
    outtextxy(500,140,"2.RED - LEVEL2");
    outtextxy(500,160,"3.GREEN - LEVEL3");
    outtextxy(500,180,"4.MAGENTA - CURED");
    outtextxy(500,200,"5.BROWN - DEATHS");

    settextstyle(SANS_SERIF_FONT,HORIZ_DIR,2);
    outtextxy(95,35,"Y");
    outtextxy(610,405,"X");
    outtextxy(85,415,"0");
    outtextxy(70,360,"60");
    outtextxy(55,300,"120");
    outtextxy(55,240,"180");
    outtextxy(55,180,"240");
    outtextxy(55,120,"300");
    switch(key)
    {
       case 1:

		outtextxy(125,425,"TAMILNADU");
		outtextxy(275,425,"KERALA");
		outtextxy(425,425,"KARNATAKA");
		settextstyle(SANS_SERIF_FONT,HORIZ_DIR,2);
		setcolor(WHITE);
		outtextxy(200,0,"STATE COMPARISON BAR CHART");
		break;
       case 2:
		outtextxy(125,425,"MADURAI");
		outtextxy(275,425,"NAGAPATTINAM");
		outtextxy(425,425,"COIMBATORE");
		settextstyle(SANS_SERIF_FONT,HORIZ_DIR,2);
		setcolor(WHITE);
		outtextxy(197,0,"DISTRICT COMPARISON BAR CHART");
		break;
       case 3:
		outtextxy(125,425,"ERNAKULAM");
		outtextxy(275,425,"ALAPPUZHA");
		outtextxy(425,425,"KOLLAM");
		settextstyle(SANS_SERIF_FONT,HORIZ_DIR,2);
		setcolor(WHITE);
		outtextxy(197,0,"DISTRICT COMPARISON BAR CHART");
		break;
       case 4:
		outtextxy(125,425,"KOLAR");
		outtextxy(275,425,"BAGALKOT");
		outtextxy(425,425,"BELAGARI");
		settextstyle(SANS_SERIF_FONT,HORIZ_DIR,2);
		setcolor(WHITE);
		outtextxy(197,0,"DISTRICT COMPARISON BAR CHART");
		break;
    }


    x_top=150;
    for(j=0;j<3;j++)
    {
	pat=pattern[0];
	col=color[0];
	setfillstyle(pat,col);
	if(j>0)
	    x_top=x_top+150;
	y_top=420-arr[j][0];
	x_bottom=x_top+50;
	y_bottom=419;
	bar_outline_x=x_top;
	bar_outline_y=y_bottom;
	bar(x_top,y_top,x_bottom,y_bottom);
	setcolor(YELLOW);
	setlinestyle(SOLID_LINE,0,3);
	line(x_top,y_top,x_bottom,y_top);
	for(i=1;i<=4;i++)
	{
		y_bottom=y_top-1;
		y_top=y_top-arr[j][i];
		col=color[i];
		pat=pattern[i];
		setfillstyle(pat,col);
		bar(x_top,y_top,x_bottom,y_bottom);
		setcolor(YELLOW);
		setlinestyle(SOLID_LINE,0,3);
		line(x_top,y_top,x_bottom,y_top);
	}
	bar_outline_x1=x_top;
	bar_outline_y1=y_top;
	line(bar_outline_x,bar_outline_y,bar_outline_x1,bar_outline_y1);
	bar_outline_x=bar_outline_x1;
	bar_outline_y=bar_outline_y1;
	bar_outline_x1=x_bottom;
	line(bar_outline_x,bar_outline_y,bar_outline_x1,bar_outline_y1);
	bar_outline_x=bar_outline_x1;
	bar_outline_y=bar_outline_y1;
	bar_outline_x1=x_bottom;
	bar_outline_y1=419;
	line(bar_outline_x,bar_outline_y,bar_outline_x1,bar_outline_y1);

    }
    getch();
    clrscr();
}

void STATE_BAR_CHART(void)
{
   int tot_lvl1=0,tot_lvl2=0,tot_lvl3=0,tot_cured=0,tot_death=0;
   int array[3][5];
   fp=fopen("madurai.txt","r");
   if(fp==NULL)
      printf("\n REQUEST FOR OPENING THE FILE MADURAI IS DECLINED");
   else
   {
      fscanf(fp,"%d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death);
      tot_lvl1=tot_lvl1+lvl1;
      tot_lvl2=tot_lvl2+lvl2;
      tot_lvl3=tot_lvl3+lvl3;
      tot_cured=tot_cured+cured;
      tot_death=tot_death+death;
      fclose(fp);
   }
   fp=fopen("nagapattinam.txt","r");
   if(fp==NULL)
      printf("\n REQUEST FOR OPENING THE FILE NAGAPATTINAM IS DECLINED");
   else
   {
      fscanf(fp,"%d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death);
      tot_lvl1=tot_lvl1+lvl1;
      tot_lvl2=tot_lvl2+lvl2;
      tot_lvl3=tot_lvl3+lvl3;
      tot_cured=tot_cured+cured;
      tot_death=tot_death+death;
      fclose(fp);
   }
   fp=fopen("coimbatore.txt","r");
   if(fp==NULL)
      printf("\n REQUEST FOR OPENING THE FILE COIMBATORE IS DECLINED");
   else
   {
      fscanf(fp,"%d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death);
      tot_lvl1=tot_lvl1+lvl1;
      tot_lvl2=tot_lvl2+lvl2;
      tot_lvl3=tot_lvl3+lvl3;
      tot_cured=tot_cured+cured;
      tot_death=tot_death+death;
      fclose(fp);
   }
   array[0][0]=tot_lvl1;
   array[0][1]=tot_lvl2;
   array[0][2]=tot_lvl3;
   array[0][3]=tot_cured;
   array[0][4]=tot_death;
   tot_lvl1=0;
   tot_lvl2=0;
   tot_lvl3=0;
   tot_cured=0;
   tot_death=0;
   fp=fopen("ernakulam.txt","r");
   if(fp==NULL)
      printf("\n REQUEST FOR OPENING THE FILE ERNAKULAM IS DECLINED");
   else
   {
      fscanf(fp,"%d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death);
      tot_lvl1=tot_lvl1+lvl1;
      tot_lvl2=tot_lvl2+lvl2;
      tot_lvl3=tot_lvl3+lvl3;
      tot_cured=tot_cured+cured;
      tot_death=tot_death+death;
      fclose(fp);
   }
   fp=fopen("alappuzha.txt","r");
   if(fp==NULL)
      printf("\n REQUEST FOR OPENING THE FILE ALAPPUZHA IS DECLINED");
   else
   {
      fscanf(fp,"%d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death);
      tot_lvl1=tot_lvl1+lvl1;
      tot_lvl2=tot_lvl2+lvl2;
      tot_lvl3=tot_lvl3+lvl3;
      tot_cured=tot_cured+cured;
      tot_death=tot_death+death;
      fclose(fp);
   }
   fp=fopen("kollam.txt","r");
   if(fp==NULL)
      printf("\n REQUEST FOR OPENING THE FILE KOLLAM IS DECLINED");
   else
   {
      fscanf(fp,"%d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death);
      tot_lvl1=tot_lvl1+lvl1;
      tot_lvl2=tot_lvl2+lvl2;
      tot_lvl3=tot_lvl3+lvl3;
      tot_cured=tot_cured+cured;
      tot_death=tot_death+death;
      fclose(fp);
   }
   array[1][0]=tot_lvl1;
   array[1][1]=tot_lvl2;
   array[1][2]=tot_lvl3;
   array[1][3]=tot_cured;
   array[1][4]=tot_death;
   tot_lvl1=0;
   tot_lvl2=0;
   tot_lvl3=0;
   tot_cured=0;
   tot_death=0;
   fp=fopen("kolar.txt","r");
   if(fp==NULL)
      printf("\n REQUEST FOR OPENING THE FILE KOLAR IS DECLINED");
   else
   {
      fscanf(fp,"%d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death);
      tot_lvl1=tot_lvl1+lvl1;
      tot_lvl2=tot_lvl2+lvl2;
      tot_lvl3=tot_lvl3+lvl3;
      tot_cured=tot_cured+cured;
      tot_death=tot_death+death;
      fclose(fp);
   }
   fp=fopen("bagalkot.txt","r");
   if(fp==NULL)
      printf("\n REQUEST FOR OPENING THE FILE BAGALKOT IS DECLINED");
   else
   {
      fscanf(fp,"%d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death);
      tot_lvl1=tot_lvl1+lvl1;
      tot_lvl2=tot_lvl2+lvl2;
      tot_lvl3=tot_lvl3+lvl3;
      tot_cured=tot_cured+cured;
      tot_death=tot_death+death;
      fclose(fp);
   }
   fp=fopen("belagari.txt","r");
   if(fp==NULL)
      printf("\n REQUEST FOR OPENING THE FILE BELAGARI IS DECLINED");
   else
   {
      fscanf(fp,"%d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death);
      tot_lvl1=tot_lvl1+lvl1;
      tot_lvl2=tot_lvl2+lvl2;
      tot_lvl3=tot_lvl3+lvl3;
      tot_cured=tot_cured+cured;
      tot_death=tot_death+death;
   }
   array[2][0]=tot_lvl1;
   array[2][1]=tot_lvl2;
   array[2][2]=tot_lvl3;
   array[2][3]=tot_cured;
   array[2][4]=tot_death;
   BAR_CHART(array,1);

}

void DISTRICT_BAR_CHART(void)
{
    int array[3][5];
    state=INFO_PRINT();
    switch(state)
    {
	case 1:
		fp=fopen("madurai.txt","r");
		if(fp==NULL)
		   printf("\n THE REQUEST TO OPEN FILE MADURAI IS DECLINED");
		else
		{
		   fscanf(fp,"%d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death);
		   array[0][0]=lvl1;
		   array[0][1]=lvl2;
		   array[0][2]=lvl3;
		   array[0][3]=cured;
		   array[0][4]=death;
		   fclose(fp);
		}
		fp=fopen("nagapattinam.txt","r");
		if(fp==NULL)
		   printf("\n THE REQUEST TO OPEN FILE NAGAPATTINAM IS DECLINED");
		else
		{
		   fscanf(fp,"%d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death);
		   array[1][0]=lvl1;
		   array[1][1]=lvl2;
		   array[1][2]=lvl3;
		   array[1][3]=cured;
		   array[1][4]=death;
		   fclose(fp);
		}
		fp=fopen("coimbatore.txt","r");
		if(fp==NULL)
		   printf("\n THE REQUEST TO OPEN FILE COIMBATORE IS DECLINED");
		else
		{
		   fscanf(fp,"%d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death);
		   array[2][0]=lvl1;
		   array[2][1]=lvl2;
		   array[2][2]=lvl3;
		   array[2][3]=cured;
		   array[2][4]=death;
		   fclose(fp);
		}
		BAR_CHART(array,2);
		break;


	case 2:
		fp=fopen("ernakulam.txt","r");
		if(fp==NULL)
		   printf("\n THE REQUEST TO OPEN FILE ERNAKULAM IS DECLINED");
		else
		{
		   fscanf(fp,"%d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death);
		   array[0][0]=lvl1;
		   array[0][1]=lvl2;
		   array[0][2]=lvl3;
		   array[0][3]=cured;
		   array[0][4]=death;
		   fclose(fp);
		}
		fp=fopen("alappuzha.txt","r");
		if(fp==NULL)
		   printf("\n THE REQUEST TO OPEN FILE ALAPPUZHA IS DECLINED");
		else
		{
		   fscanf(fp,"%d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death);
		   array[1][0]=lvl1;
		   array[1][1]=lvl2;
		   array[1][2]=lvl3;
		   array[1][3]=cured;
		   array[1][4]=death;
		   fclose(fp);
		}
		fp=fopen("kollam.txt","r");
		if(fp==NULL)
		   printf("\n THE REQUEST TO OPEN FILE KOLLAM IS DECLINED");
		else
		{
		   fscanf(fp,"%d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death);
		   array[2][0]=lvl1;
		   array[2][1]=lvl2;
		   array[2][2]=lvl3;
		   array[2][3]=cured;
		   array[2][4]=death;
		   fclose(fp);
		}
		BAR_CHART(array,3);
		break;
	case 3:
		fp=fopen("kolar.txt","r");
		if(fp==NULL)
		   printf("\n THE REQUEST TO OPEN FILE KOLAR IS DECLINED");
		else
		{
		   fscanf(fp,"%d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death);
		   array[0][0]=lvl1;
		   array[0][1]=lvl2;
		   array[0][2]=lvl3;
		   array[0][3]=cured;
		   array[0][4]=death;
		   fclose(fp);
		}
		fp=fopen("bagalkot.txt","r");
		if(fp==NULL)
		   printf("\n THE REQUEST TO OPEN FILE BAGALKOT IS DECLINED");
		else
		{
		   fscanf(fp,"%d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death);
		   array[1][0]=lvl1;
		   array[1][1]=lvl2;
		   array[1][2]=lvl3;
		   array[1][3]=cured;
		   array[1][4]=death;
		   fclose(fp);
		}
		fp=fopen("belagari.txt","r");
		if(fp==NULL)
		   printf("\n THE REQUEST TO OPEN FILE BELAGARI IS DECLINED");
		else
		{
		   fscanf(fp,"%d %d %d %d %d",&lvl1,&lvl2,&lvl3,&cured,&death);
		   array[2][0]=lvl1;
		   array[2][1]=lvl2;
		   array[2][2]=lvl3;
		   array[2][3]=cured;
		   array[2][4]=death;
		   fclose(fp);
		}
		BAR_CHART(array,4);
		break;

	default:
		printf("\N ENTER A VALID STATE ID BETWEEN 1-3");
		getch();
		DISTRICT_BAR_CHART();

    }
}
void ASSIGN(void)
{
   int temp;
   lvl1=20;lvl2=10;lvl3=5;cured=10;death=5;vent=20,population=200;
   temp=lvl1+lvl2+lvl3;
   percent=temp*100;
   percent=percent/(float)population;
   fp=fopen("epasslst.txt","w");
   fclose(fp);
   fp=fopen("madurai.txt","w+");
   if(fp==NULL)
   {
      printf("\n REQUEST FOR OPENING FILE MADURAI IS DECLINED");
   }
   else
   {
      FILE_WRITE_DATA();
      fclose(fp);
   }
   fp=fopen("nagapattinam.txt","w+");
   if(fp==NULL)
     printf("\n REQUEST FOR OPENING FILE NAGAPATTINAM IS DECLINED");
   else
   {
      FILE_WRITE_DATA();
      fclose(fp);
   }
   fp=fopen("coimbatore.txt","w+");
   if(fp==NULL)
     printf("\n REQUEST FOR OPENING FILE COIMBATORE IS DECLINED");
   else
   {
      FILE_WRITE_DATA();
      fclose(fp);
   }
   fp=fopen("ernakulam.txt","w+");
   if(fp==NULL)
     printf("\n REQUEST FOR OPENING FILE ERNAKULAM IS DECLINED");
   else
   {
      FILE_WRITE_DATA();
      fclose(fp);
   }
   fp=fopen("alappuzha.txt","w+");
   if(fp==NULL)
     printf("\n REQUEST FOR OPENING FILE ALAPPUZHA IS DECLINED");
   else
   {
      FILE_WRITE_DATA();
      fclose(fp);
   }
   fp=fopen("kollam.txt","w+");
   if(fp==NULL)
     printf("\n REQUEST FOR OPENING FILE KOLLAM IS DECLINED");
   else
   {
      FILE_WRITE_DATA();
      fclose(fp);
   }
   fp=fopen("kolar.txt","w+");
   if(fp==NULL)
     printf("\n REQUEST FOR OPENING FILE KOLAR IS DECLINED");
   else
   {
      FILE_WRITE_DATA();
      fclose(fp);
   }
   fp=fopen("bagalkot.txt","w+");
   if(fp==NULL)
     printf("\n REQUEST FOR OPENING FILE BAGALKOT IS DECLINED");
   else
   {
      FILE_WRITE_DATA();
      fclose(fp);
   }
   fp=fopen("belagari.txt","w+");
   if(fp==NULL)
     printf("\n REQUEST FOR OPENING FILE BELAGARI IS DECLINED");
   else
   {
      FILE_WRITE_DATA();
      fclose(fp);
   }
}



void READDATA (void)
{
    state=INFO_PRINT();
    switch(state)
    {
       case 1 :
		dist=GET_DIST(state);
		switch(dist)
		{
		   case 1:
			   clrscr();
			   printf("\t\t\t STATE 1 : TAMILNADU---DISTRICT 1: MADURAI");
			   GET_INFO();
			   fp=fopen("madurai.txt","w+");
			   if(fp==NULL)
			      printf("\n REQUEST FOR OPENING FILE MADURAI IS DECLINED");
			   else
			   {
			       FILE_WRITE_DATA();
			       fclose(fp);
			   }
			   break;
		   case 2:
			   clrscr();
			   printf("\t\t\t STATE 1 : TAMILNADU---DISTRICT 2: NAGAPATTINAM");
			   GET_INFO();
			   fp=fopen("nagapattinam.txt","w+");
			   if(fp==NULL)
			      printf("\n REQUEST FOR OPENING FILE NAGAPATTINAM IS DECLINED");
			   else
			   {

			       FILE_WRITE_DATA();
			       fclose(fp);
			   }
			   break;
		   case 3:
			   clrscr();
			   printf("\t\t\t STATE 1 : TAMILNADU---DISTRICT 3: COIMBATORE");
			   GET_INFO();
			   fp=fopen("coimbatore.txt","w+");
			   if(fp==NULL)
			      printf("\n REQUEST FOR OPENING FILE COIMBATORE IS DECLINED");
			   else
			   {
			       FILE_WRITE_DATA();
			       fclose(fp);
			   }
			   break;
		   default: printf("\n ENTER A VALID DISTRICT ID BETWEEN 1-3");
			    getch();
			    READDATA();
		}
		break;
       case 2:
		dist=GET_DIST(state);
		switch(dist)
		{
		   case 1:
			   clrscr();
			   GET_INFO();
			   fp=fopen("ernakulam.txt","w+");
			   if(fp==NULL)
			      printf("\n REQUEST FOR OPENING FILE ERNAKULAM IS DECLINED");
			   else
			   {
			       FILE_WRITE_DATA();
			       fclose(fp);
			   }
			   break;
		   case 2:
			   clrscr();
			   printf("\t\t\t STATE 2 : KERALA---DISTRICT 2: ALAPPUZHA");
			   GET_INFO();
			   fp=fopen("alappuzha.txt","w+");
			   if(fp==NULL)
			      printf("\n REQUEST FOR OPENING FILE ALAPPUZHA IS DECLINED");
			   else
			   {
			       FILE_WRITE_DATA();
			       fclose(fp);
			   }
			   break;
		   case 3:
			   clrscr();
			   printf("\t\t\t STATE 1 : KERALA---DISTRICT 3: KOLLAM");
			   GET_INFO();
			   fp=fopen("kollam.txt","w+");
			   if(fp==NULL)
			      printf("\n REQUEST FOR OPENING FILE KOLLAM IS DECLINED");
			   else
			   {
			       FILE_WRITE_DATA();
			       fclose(fp);
			   }
			   break;
		   default: printf("\n ENTER A VALID DISTRICT ID BETWEEN 1-3");
			    getch();
			    READDATA();
		}
		break;
       case 3:
		dist=GET_DIST(state);
		switch(dist)
		{
		   case 1:
			   clrscr();
			   printf("\t\t\t STATE 3 : KARNATAKA---DISTRICT 1: KOLAR");
			   GET_INFO();
			   fp=fopen("kolar.txt","w+");
			   if(fp==NULL)
			      printf("\n REQUEST FOR OPENING FILE KOLAR IS DECLINED");
			   else
			   {
			       FILE_WRITE_DATA();
			       fclose(fp);
			   }
			   break;
		   case 2:
			   clrscr();
			   printf("\t\t\t STATE 3 : KARNATAKA---DISTRICT 2: BAGALKOT");
			   GET_INFO();
			   fp=fopen("bagalkot.txt","w+");
			   if(fp==NULL)
			      printf("\n REQUEST FOR OPENING FILE BAGALKOT IS DECLINED");
			   else
			   {
			       FILE_WRITE_DATA();
			       fclose(fp);
			   }
			   break;
		   case 3:
			   clrscr();
			   printf("\t\t\t STATE 1 : KARNATAKA---DISTRICT 3: BELAGARI");
			   GET_INFO();
			   fp=fopen("belagari.txt","w+");
			   if(fp==NULL)
			      printf("\n REQUEST FOR OPENING FILE BELAGARI IS DECLINED");
			   else
			   {
			       FILE_WRITE_DATA();
			       fclose(fp);
			   }
			   break;
		   default: printf("\n ENTER A VALID DISTRICT ID BETWEEN 1-3");
			    getch();
			    READDATA();
		}
		break;
       default: printf("\nPLEASE ENTER A VALID STATE ID BETWEEN 1-3");
		getch();
		READDATA();
    }
}

void INFORMATION(void)
{
    state=INFO_PRINT();
    switch(state)
    {
       case 1 :
		clrscr();
		printf("--------------------------------------------------------------------------------");
		printf("\n\t\t\tSTATE 3 : TAMIL NADU\n");
		printf("--------------------------------------------------------------------------------");
		printf("\n1.MADURAI\n2.NAGAPATTINAM\n3.COIMBATORE\n4.ALL\n");
