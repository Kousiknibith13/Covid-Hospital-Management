void READDATA (void)

{
    int state,dist,lvl1,lvl2,lvl3,cured,death,vent;
clrscr();
printf("-----------------------------------\n");
printf("1.TAMILNADU\n2.KERALA\n3.KARNATAKA\n");
printf("-----------------------------------\n");
printf("ENTER STATE ID: ");
scanf("%d",&state);
    switch(state)
    {
       case 1 :
		clrscr();
		printf("\n------------------------------------");
		printf("\n\t\t\tSTATE 1 : TAMIL NADU");
		printf("\n------------------------------------");
		printf("\n1.MADURAI\n2.NAGAPATTINAM\n3.COIMBATORE");
		printf("\n---------------------------------------");
		printf("\n ENTER DISTRICT ID : ");
		scanf("%d",&dist);
		switch(dist)
		{
		   case 1:
			clrscr();
			printf("\t\t\t STATE 1 : TAMILNADU---DISTRICT 1: MADURAI");
			printf("\n ENTER THE NUMBER OF PATIENTS RECORDED IN LEVEL 1 : ");
			scanf("%d",&lvl1);
			printf("\n ENTER THE NUMBER OF PATIENTS RECORDED IN LEVEL 2 : ");
			scanf("%d",&lvl2);
			printf("\n ENTER THE NUMBER OF PATIENTS RECORDED IN LEVEL 3 : ");
			scanf("%d",&lvl3);
			printf("\n ENTER THE NUMBER OF PATIENTS CURED : ");
			scanf("%d",&cured);
			printf("\n ENTER THE NUMBER OF PATIENTS DEAD : ");
			scanf("%d",&death);
			printf("\n ENTER THE NUMBER OF VENTILATORS AVAILABLE : ");
			scanf("%d",&vent);
			fp=fopen("madurai.txt","w+");
			   if(fp==NULL)
			printf("\n REQUEST FOR OPENING FILE MADURAI IS DECLINED");
			   else
			   {
			fprintf(fp,"%d",lvl1);
			fprintf(fp,"\n");
			fprintf(fp,"%d",lvl2);
			fprintf(fp,"\n");
			fprintf(fp,"%d",lvl3);
			fprintf(fp,"\n");
